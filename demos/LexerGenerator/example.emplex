-whitespace : [ \t\n\r]+
-comment    : #.*
integer     : [0-9]+
float       : ([0-9]+.[0-9]*)|(.[0-9]+)
keyword     : (break)|(continue)|(else)|(for)|(if)|(return)|(while)
identifier  : [a-zA-Z_][a-zA-Z0-9_]*
string      : (\"([^"\\]|(\\.))*\")|('([^'\\]|(\\.))*')
symbol      : .|"::"|"=="|"!="|"<=|">="|"->"|"&&"|"||"|"<<"|">>"|"++"|"--"|"**"

